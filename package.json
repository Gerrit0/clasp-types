{
  "name": "clasp-types",
  "version": "1.0.0",
  "description": "A Typescript definitions generator for clasp projects",
  "homepage": "https://github.com/maelcaldas/clasp-types#readme",
  "main": "./dist/index.js",
  "bin": {
    "clasp-types": "./dist/index.js"
  },
  "author": "Mael Caldas (https://github.com/maelcaldas)",
  "repository": {
    "type": "git",
    "url": "https://github.com/maelcaldas/clasp-types.git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "prepublish": "yarn build",
    "prebuild": "copyfiles ./src/lib/builders/*.json -f dist/lib/builders/",
    "build": "tsc -p .",
    "watch": "yarn build -w",
    "predeploy": "yarn build",
    "deploy": "npm publish --dry-run"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/node": "^12.7.5",
    "copyfiles": "^2.1.1",
    "nodemon": "^1.19.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "commander": "^3.0.1",
    "fs-extra": "^8.1.0",
    "typedoc": "^0.15.0"
  }
}
